{
  "name": "vite-scaffold",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "serve-clean": "rm -rf dist && npm run build && npm run serve",
    "clean:build": "rm -rf dist && npm run build",
    "lint": "eslint src/**",
    "format": "prettier --write src/**",
    "json-server": "json-server --watch --delay 1000 --port 3001 db.json"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.3.0",
    "json-server": "^0.17.0",
    "prettier": "^2.4.1",
    "vite": "^2.6.4"
  },
  "dependencies": {
    "lib0": "^0.2.42",
    "lit-html": "^2.0.2",
    "xstate": "^4.26.1",
    "y-webrtc": "^10.2.0",
    "y-websocket": "^1.3.17",
    "yjs": "^13.5.18"
  }
}
